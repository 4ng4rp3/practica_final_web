{
  "name": "vue-phaser3",
  "description": "Template for working with Phaser 3 in a Vue project",
  "keywords": [
    "vue",
    "phaser",
    "boilerplate"
  ],
  "version": "1.1.0",
  "author": "Markus Appel <markospoparkos@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sun0fABeach/vue-phaser3.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --watch --config webpack.dev.js",
